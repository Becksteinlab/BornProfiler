ELEC name %(stage)s
  mg-auto
  mol %(molnum)d
  dime %(DIME_XYZ)s
  cglen %(CGLEN_XYZ)s
  fglen 20 20 40    
  cgcent mol 3
  fgcent mol 2
  # NaCl ionic strength in mol/L
  ion  charge  1   conc %(conc).2f  radius 0.95   # sodium ions
  ion  charge -1   conc %(conc).2f  radius 1.81   # chloride ions
  lpbe
  bcfl mdh
  pdie 2.0 # protein and faux-lipid
  sdie 78.5 # Eisenberg and Crothers Phys. Chem. book 1979
  srfm smol
  chgm spl2
  srad 1.4
  swin 0.3
  sdens 10.0
  temp %(temperature).2f
  calcenergy total
  calcforce no
END
